{
  "name": "@v-bible/types",
  "version": "0.6.2",
  "author": "v-bible",
  "description": "v-bible backend types",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/v-bible/backend.git"
  },
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    }
  },
  "scripts": {
    "lint": "tsc",
    "build": "tsup",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "gen-barrel": "barrelsby --directory lib --exclude index.ts --delete",
    "prepublish": "pnpm run gen-barrel && pnpm run build && pnpm run check-exports && pnpm run lint"
  },
  "dependencies": {
    "@bufbuild/buf": "1.56.0",
    "@bufbuild/protobuf": "2.7.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "barrelsby": "2.8.1",
    "tsup": "8.5.0",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@bufbuild/buf"
    ]
  }
}
